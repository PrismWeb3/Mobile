{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@navigation": [
        "src/navigation"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@screens": [
        "src/screens"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@services": [
        "src/services"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@types": [
        "src/types"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles": [
        "src/styles"
      ],
      "@globals/*": [
        "src/globals/*"
      ],
      "@globals": [
        "src/globals"
      ]
    }
  }
}